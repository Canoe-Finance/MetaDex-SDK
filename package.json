{
  "name": "metadex-sdk",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@git.arche.network:front-web/metadex/metadex-sdk.git",
  "author": "Clayon <admin@github.xyz>",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "lint:ts": "eslint --fix --ext .ts src",
    "lint:css": "stylelint --fix \"./src/**/*.{css,scss}\"",
    "format": "prettier --write './src/**/*.{ts,scss}' --list-different --ignore-path .gitignore ."
  },
  "dependencies": {
    "chart.js": "2.8.0",
    "ethereum-input-data-decoder": "^0.4.0",
    "ethers": "^5.6.1",
    "lit": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/chart.js": "^2.9.35",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "rollup-plugin-postcss-lit": "^2.0.0",
    "sass": "^1.49.0",
    "stylelint": "^14.3.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:ts",
    "*.{css,scss}": "npm run lint:css",
    "*.{js,jsx,ts,tsx,css,scss,json,md}": "npm run format"
  }
}
